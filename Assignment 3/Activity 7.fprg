    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="This program displays your dogs age in puppy years when given human years&#13;&#10;"/>
            <declare name="DogsName" type="String" array="False" size=""/>
            <declare name="DogsAge" type="Integer" array="False" size=""/>
            <declare name="DogHumanAge" type="Integer" array="False" size=""/>
            <output expression="&quot;What is your Dog's name?&quot;" newline="True"/>
            <input variable="DogsName"/>
            <output expression="&quot;How old is your dog, in human years?&quot;" newline="True"/>
            <input variable="DogsAge"/>
            <assign variable="DogHumanAge" expression="DogsAge * 7"/>
            <output expression="DogsName &amp; &quot; is &quot; &amp; DogHumanAge &amp; &quot; years young, in human years! &quot;" newline="True"/>
        </body>
    </function>
