// check output statement
#include <iostream>
using namespace std; 


int main(){
    const int employees = 2;
    int salaries[8] = {0};
    float gross;
    int comission;
    
    for (int i = 0; i < employees; i++){
        cout << "Enter gross pay for salesperson #"
        << i + 1 << ": ";
        cin >> gross;
        cin.ignore();
        gross = gross * .09;
        comission = gross / 100;
        salaries[comission]++;
    }
    for (int i = 1; i < 9; i++){
        cout << "The number of salespeople earning in range $" << 
        ((i - 1) * 100) + 200 << " - " << ((i * 100) + 200) - 1 << ": " << salaries[i - 1] << endl;
    }
    cout << "The number of salespeople earning in range $1000 and more " << salaries[8] << endl; 
}
